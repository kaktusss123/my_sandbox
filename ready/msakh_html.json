  ],
  "_segment": "",
  "_operation": "",
  "_federal_subject": "",
  "_source": "MSAKH",
  "global_settings": {
    "CONCURRENT_REQUESTS": 256,
    "DOWNLOAD_TIMEOUT": 60,
    "SPLASH_URL": "http://0.0.0.0:8050",
    "LOG_LEVEL": "INFO",
    "AUTOTHROTTLE_ENABLED": false,
    "AUTOTHROTTLE_START_DELAY": 1.0,
    "AUTOTHROTTLE_MAX_DELAY": 60,
    "AUTOTHROTTLE_TARGET_CONCURRENCY": 256,
    "HTTPERROR_ALLOWED_CODES": [],
    "CONCURRENT_REQUESTS_PER_DOMAIN": 256,
    "RETRY_ENABLED": true,
    "RETRY_HTTP_CODES": [
      500,
      503,
      502,
      504,
      400,
      403,
      408,
      429,
      505,
      303,
      302,
      301,
      404,
      307,
      410,
      502,
      451
    ],
    "RETRY_TIMES": 20,
    "ROBOTSTXT_ENABLED": false,
    "ROBOTSTXT_OBEY": false,
    "REDIRECT_ENABLED": false,
    "COOKIES_ENABLED": false,
    "RANDOM_UA_TYPE": "random"
  },
  "methods": {
    "crawling_type": "common",
    "allowed_domains": [],
    "proxy_settings": {
      "types": [
        "https"
      ]
    }
  },
  "crawl_tree": [
    {
      "pagination_urls": {
        "path": "//span[@class ='item selected']/following-sibling::span[1]/a/@href"
      },
      "item_urls": {
        "path": "//div[contains(@class, 'snapshot')]/a/@href",
        "methods":{"save_cookies": false, "filter_duplicates": false}
      }
    }
  ],
  "item_page": {
    "fields": {
      "federal_subject": {
        "type": "string",
        "value": "Сахалинская обл"
      },
      "title_name": {
        "type": "string",
        "path": "//div[contains(@id, 'offer')]/h1/text()"
      },
      "date_info": {
        "type": "date",
        "time": "now"
      },
      "date_created": {
        "type": "date",
        "path": "(//div[contains(@class, 'stat')]/div/text())[1]",
        "time": "from string",
        "regex": "(?<=обавлено )([^;]*)(?=\\,)"
      },
      "date_updated": {
        "type": "date",
        "path": "(//div[contains(@class, 'stat')]/div/text())[1]",
        "time": "from string",
        "regex": "(?<=новлено )([^;]*)(?=\\,)"
      },
      "description": {
        "type": "list to string",
        "path": "//div[@class = 'fulltext']/text()"
      },
      "price_total": {
        "type": "float",
        "path": "//div[@class = 'price']/div[@class = 'sum']/span/text()",
        "regex": "\\d+"
      },
      "area_total": {
        "type": "float",
        "regex": "\\d+[\\,\\.]{0,1}\\d{1,100}(?= м)",
        "path": "(//div[@class = 'area']/text())[1]"
      },
      "address": {
        "type": "list to string",
        "path": "//div[@id='offer']/h4[contains(text(),',')]/text()|//div[@id='offer']/h4[contains(text(),',')]/a/span[1]/text()"
      },
      "address_city": {
        "type": "list to string",
        "path": "//div[@class = 'currentCity']/text()"
      },
      "operation": {
        "type": "string",
        "path": "//div[contains(@id, 'offer')]/h1/text()",
        "regex": "Сдам|Продам"
      },
      "segment": {
        "type": "string",
        "path": "//div[contains(@id, 'offer')]/h1/text()",
        "regex": "коммерческ"
      },
      "purpose": {
        "type": "string",
        "path": "//div[contains(@id, 'offer')]/h3/text()"
      },
      "floor": {
        "type": "string",
        "path": "(//div[@class = 'area']/text())[2]",
        "regex": "(?<=Этаж: )\\d+(?=\\/)"
      },
      "floors_cnt": {
        "type": "string",
        "path": "(//div[@class = 'area']/text())[2]",
        "regex": "(?<=\\/)\\d+"
      },
      "seller_name": {
        "type": "string",
        "path": "//div[@class='name']/a/text()|//div[@class='name']/text()"
      }
    },
    "tables": {
      "table_1": {
        "_keys_path": "//*[@class='params' ]//text()[contains(., ':')]",
        "_vals_path": "//*[@class='params' ]//text()[contains(., ':')]",
        "key_regex": ".+(?=:)",
        "key_converter": {
          "год_постройки": {
            "type": "float",
            "name": "year_built",
            "regex": "\\d+"
          },
          "электричество'": {
            "type": "string",
            "name": "is_electricity",
            "regex": "(?<=:).+"
          },
          "отопление": {
            "type": "string",
            "name": "is_heating",
            "regex": "(?<=:).+"
          },
          "водоснабжение": {
            "type": "string",
            "name": "is_plumbing",
            "regex": "(?<=:).+"
          },
          "канализация": {
            "type": "string",
            "name": "is_sewage",
            "regex": "(?<=:).+"
          },
          "охрана": {
            "type": "string",
            "name": "is_security",
            "regex": "(?<=:).+"
          },
          "материал_стен": {
            "type": "string",
            "name": "walls_material",
            "regex": "(?<=:).+"
          },
          "коммуникации": {
            "type": "string",
            "name": "is_communications",
            "regex": "(?<=:).+"
          },
          "ипотека": {
            "type": "string",
            "name": "is_mortage",
            "regex": "(?<=:).+"
          }
        }
      }
    },
    "follow": [
      {
        "fields": {
          "phone": {
            "type": "string",
            "path": "html",
            "regex": "(?<=\">)[0-9-()+]+(?=</a></em>)"
          }
        },
        "post_params": {
          "fields": {
            "action": {
              "type": "string",
              "value": "get-phone"
            },
            "id": {
              "type": "string",
              "path": "//div[@id='how-to-contact']/@data-id"
            },
            "type": {
              "type": "string",
              "path": "//div[@id='how-to-contact']/@data-type"
            },
            "key": {
              "type": "string",
              "path": "//div[@id='how-to-contact']/@data-key"
            }
          }
        },
        "headers": {
          "x-requested-with": {
            "type": "string",
            "value": "XMLHttpRequest"
          },
          "accept":{
            "type": "string",
            "value": "application/json, text/javascript, */*; q=0.01"
          }
        },
          "methods": {
            "crawling_type": "form_post",
            "save_cookies": false,
            "content_type": "json",
            "filter_duplicates": false,
            "allowed_domains": [],
            "proxy_settings": {
              "types": [
                "https"
              ],
              "amount": 10000,
              "save": false
            }
          },
          "url": "https://dom.sakh.com/usrajax.php"
        }
        ]
      }
    }


