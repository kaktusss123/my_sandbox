      },
      "date_info": {
        "type": "date",
        "time": "now"
      },
      "date_created": {
        "type": "date",
        "path": "//span[@class = 'submitted-by']/text()",
        "time": "from string",
        "regex": "(?<=ны: )([^;]*)(?= )"
      },
      "date_updated": {
        "type": "date",
        "path": "//div[@class='content clearfix']/span[1]/text()",
        "time": "from string",
        "regex": "(?<=ны: )[^;]*"
      },
      "description": {
        "type": "list to string",
        "path": "//div[@class = 'field-item even']/p/text()"
      },
      "price_total": {
        "type": "float",
        "path": "//div[contains(@class, 'price')]//div[@class='field-item even']/text()",
        "regex": "\\d*\\W?\\d+"
      },
      "price_currency": {
        "type": "float",
        "path": "//div[contains(@class, 'price')]//div[@class='field-item even']/text()",
        "regex": "[А-Яа-я ]+"
      },
      "source_latitude": {
        "type": "float",
        "path": "//script[contains(text(),'yandex#map')]/text()",
        "regex": "(?<=\"center\":\\[)\\d+\\W?\\d+"
      },
      "source_longitude": {
        "type": "float",
        "path": "//script[contains(text(),'yandex#map')]/text()",
        "regex": "\\d+\\W?\\d+(?=\\],\"zoom\":)"
      },
      "seller_name": {
        "type": "string",
        "path": "//ul[@class='list-agent-info']/li[@class='views-field-title']//text()[normalize-space()]"
      },
      "seller_company": {
        "type": "string",
        "path": "//div[contains(@id,'agent')]/h2/text()"
      },
      "phone": {
        "type": "phone",
        "path": "//div[contains(@id,'agent')]//i[contains(@class,'phone')]/parent::li/text()[normalize-space()]"
      }
    },
