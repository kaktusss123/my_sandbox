                "operations": []
            },
            "date_created": {
                "type": "date",
                "path": "//p[contains(text(),'Добавлено')]/text()",
                "time": "from string",
                "regex": "\\d\\d.\\d\\d.\\d\\d\\d\\d",
                "operations": []
            },
            "title_name": {
                "type": "string",
                "path": "//span[@itemprop='name']/text()",
                "operations": []
            },
            "price_total": {
                "type": "float",
                "path": "//span[@itemprop='price']/text()",
                "regex": "\\d+",
                "operations": []
            },
            "price_currency": {
                "type": "string",
                "path": "//span[@itemprop='price']/../text()",
                "operations": []
            },
            "description": {
                "type": "list to string",
                "path": "//h2[@class='h2_under']/following-sibling::p/text()",
                "operations": []
            }
        },
        "follow": [{
            "url_constructor": {
                "add": "/?print2"
            },
            "fields": {
                "phone": {
                    "type": "phone",
                    "path": "//td[contains(text(),'Тел')]/following-sibling::td/b/text()"
                },
                "seller_name": {
                    "type": "string",
                    "path": "//td[@style='vertical-align: top; font-size: 20px; ']/b/text()"
                },
                "address": {
                    "type": "string",
                    "path": "//td/p/text()",
                    "operations": []
                }

