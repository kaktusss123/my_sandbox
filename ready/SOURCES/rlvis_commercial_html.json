                "operations": []
            },
            "floors_cnt": {
                "type": "float",
                "path": "//td[contains(text(),'Этаж')]/following-sibling::td[1]/text()",
                "regex": "(?<=\/)(.*)",
                "operations": []
            },            
            "date_info": {
                "type": "date",
