    {
      "pagination_urls": {
        "path": "//li[contains(@class, 'pagination__pagesItem_active')]/following-sibling::li[1]/a/@href"
      },
      "item_urls": {
        "path": "//div[@class='listing js-productGrid ']//div[@class='listing__itemTitleWrapper']/a/@href"
      }
    }
  ],
  "item_page": {
    "fields": {
      "date_info": {
        "type": "date",
        "time": "now"
      },
      "federal_subject": {
        "type": "string",
        "regex": "\"address_region\":\"([\\w -]+)\""

      },
      "address_city": {
        "type": "string",
        "regex": "\"address_city\":\"([\\w -]+)\""
        
      },
      "currency": {
        "type": "string",
        "path": "//meta[@itemprop='priceCurrency']/@content"
      },
      "title_name": {
        "type": "string",
        "path": "//title/text()"
      },
      "address": {
        "type": "string",
        "path": "//div[@class='productPage__infoTextBold js-scrollToMap']/text()"
      },
      "description": {
        "type": "list to string",
        "path": "//p[@itemprop='description']/text()"
      },
      "seller_name": {
        "type": "string",
        "path": "normalize-space((//div[@class='productPage__infoTextBold productPage__infoTextBold_inline'][count(./*)=1]/a | //div[@class='productPage__infoTextBold productPage__infoTextBold_inline'][count(./*)=0])/text())"
      },
      "seller_type": {
        "type": "string",
        "path": "//a[@class='js-sellerSiteLink']/text()"
        
      },
      "phone": {
        "type": "phone_bs64",
        "path": "//input[@name='phoneBase64']/@value"
      },
      "date_created": {
        "type": "date",
        "path": "//div[@class='productPage__createDate']/span/text()",
        "time": "from string"
      },
      "price_total": {
        "type": "float",
        "path": "//div[@itemprop='price']/@content"
      },
      "price_term": {
        "type": "string",
        "path": "//div[@itemprop='price']/text()"
        
      },
      "price_unit": {
        "type": "string",
        "path": "//div[@itemprop='price']/text()"
        
      },
      "source_latitude": {
        "type": "float",
        "path": "//div[@class='js-productPageMap']/@data-map-info",
        "regex": "(?<=lat\":\")\\d*.\\d+"
        
      },
      "source_longitude": {
        "type": "float",
        "path": "//div[@class='js-productPageMap']/@data-map-info",
        "regex": "(?<=lng\":\")\\d*.\\d+"
        
      },
      "agent_fee": {
        "type": "string",
        "path": "//div[@class='productPage__fee']/text()",
        "regex": "\\d*\\W?\\D+$"
        
      },
      "segment": {
        "type": "string",
        "path": "//ul[@class='breadcrumbs__list js-breadcrumbsNav']//li[4]//span/text()"
      },
      "operation": {
        "type": "string",
        "path": "//ul[@class='breadcrumbs__list js-breadcrumbsNav']//li[3]//span/text()",
        "regex": "Продажа|Аренда"
      }
    },
    "tables": {
      "table_1": {
        "_keys_path": "//li[@class='productPage__infoColumnBlockText']/text()",
        "_vals_path": "//li[@class='productPage__infoColumnBlockText']/text()",
        "key_regex": ".+?(?=:.)|^.+$",
        "key_converter": {
            "строения": {
            "name": "building_type",
            "type": "string",
