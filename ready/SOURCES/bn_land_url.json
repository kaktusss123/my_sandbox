  "crawl_tree": [
    {
      "pagination_urls": {
        "path": "//li[@class=\"current-page disabled\"]/following-sibling::li/a/@href|//a[@class=\"active\"]/following-sibling::a/@href[normalize-space()]"
      },
      "item_urls": {
        "path": "//tr[@onclick=\"tr_click(this)\"]//a/@href|//a[@class=\"object--link-to-object button_mode_one\"]/@href[normalize-space()]"
      }
    }
  ],
  "item_page": {
    "fields": {
      "price_total": {
        "type": "float",
        "path": "//span[@class=\"cost\"]/text()[normalize-space()]|//div[@class=\"object-first-info_item last\"]/div[@class=\"object-first-info_item_big\"]/text()[normalize-space()]",
        "regex": "[\\d \\.]+"
      },
      "seller_name": {
        "type": "list to string",
        "path": "//dl[@class=\"sell\"]/dd//text()[normalize-space()]|//div[@class=\"person-info_line_val\"][1]//text()[normalize-space()]"
      },
      "phone": {
        "type": "phone",
        "path": "//dt[contains(text(), \"Телефон\")]/following-sibling::dd/text()[normalize-space()]"
      },
      "title": {
        "type": "string",
        "path": "//section[@class=\"round_gr detail\"]/h1/text()[normalize-space()]|//div[@class=\"object-head\"][1]/h1/text()[normalize-space()]"
      },
      "federal_subject": {
        "type": "string",
        "path": "//dt[contains(text(), \"Регион\")]/following-sibling::dd/text()[normalize-space()]"
      },
      "address": {
        "type": "list to string",
        "path": "//td[contains(text(), \"Адрес\")]/following-sibling::td/text()[normalize-space()]|//div[contains(text(), \"Адрес\")]/following-sibling::div/text()[normalize-space()]"
      },
      "source_latitude": {
        "type": "float",
        "path": "//img[@id=\"print_map\"]/@src[normalize-space()]|//div[@class=\"object-map\"]/img/@src",
        "regex": "(?<=ll=)[\\d\\.]+"
      },
      "source_longitude": {
        "type": "float",
        "path": "//img[@id=\"print_map\"]/@src[normalize-space()]|//div[@class=\"object-map\"]/img/@src",
        "regex": "[\\d\\.]+(?=&size)"
      },
      "land_area": {
        "type": "float",
        "path": "//dt[contains(text(), \"Площадь участка\")]/following-sibling::dd/text()[normalize-space()]|//div[contains(text(), \"Площадь участка\")]/following-sibling::div/text()[normalize-space()]",
        "regex": "[\\d\\.]{2,}|\\d"
      },
      "area_total": {
        "type": "float",
        "path": "//dt[contains(text(), \"Общая площадь\")]/following-sibling::dd/text()",
        "regex": "[\\d\\.]{2,}|\\d"
      },
      "area_land_unit": {
        "type": "string",
        "path": "//dt[contains(text(), \"Площадь участка\")]/following-sibling::dd/text()[normalize-space()]|//div[contains(text(), \"Площадь участка\")]/following-sibling::div/text()[normalize-space()]",
        "regex": "[А-Яа-я ]+",
        "string_operations": [
          "lower"
        ]
      },
      "purpose": {
        "type": "string",
        "path": "//dt[contains(text(), \"Статус участка\")]/following-sibling::dd/text()"
      },
      "date_created": {
        "type": "date",
        "time": "from string",
        "path": "//dt[contains(text(), \"Дата размещения\")]/following-sibling::dd/text()[normalize-space()]"
      },
      "date_updated": {
        "type": "date",
        "time": "from string",
        "path": "//dt[contains(text(), \"Дата обновления\")]/following-sibling::dd/text()[normalize-space()]"
      },
      "date_info": {
        "type": "date",
        "time": "now"
      },
      "description": {
        "type": "list to string",
        "path": "//div[@id=\"description\"]//text()[normalize-space()]|//div[@class=\"object-text\"]/p/text()[normalize-space()]"
      },
      "land_type": {
        "type": "string",
        "path": "//div[@class=\"object-description\"]//div[contains(text(), \"Статус земли\")]/following-sibling::div/text()"
      },
      "operation": {
        "type": "string",
        "path": "//section[@class=\"round_gr detail\"]/h1/text()[normalize-space()]|//div[@class=\"object-head\"][1]/h1/text()[normalize-space()]",
        "regex": "[Пп]род|[Аа]ренд"
      }
    }
  }
