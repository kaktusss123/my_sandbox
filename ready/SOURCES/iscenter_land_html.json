      },
      "area_land_unit": {
        "type": "string",
        "path": "//td[contains(text(), \"Площадь участка\")]/following-sibling::td/text()[normalize-space()]",
        "regex": "[А-Яа-я ]+"
      },
      "address": {
        "type": "string",
        "path": "//td[contains(text(), \"Адрес\")]/following-sibling::td/text()[normalize-space()]"        
      },
      "address_city": {
        "type": "string",
        "path": "//td[contains(text(), \"Город\")]/following-sibling::td/text()[normalize-space()]"        
      },
      "date_created": {
        "type": "date",
        "time": "from string",
        "path": "//td[contains(text(), \"Дата подачи объявления\")]/following-sibling::td/text()[normalize-space()]"        
      },
      "date_updated": {
        "type": "date",
        "time": "from string",
        "path": "//td[contains(text(), \"Дата обновления\")]/following-sibling::td/text()[normalize-space()]"        
      },
      "metro": {
        "type": "string",
        "path": "//td[contains(text(), \"Метро\")]/following-sibling::td/text()[normalize-space()]"        
      },
      "address_district": {
        "type": "string",
        "path": "//td[contains(text(), \"Район\")]/following-sibling::td/text()[normalize-space()]"        
      }
    }
  }
