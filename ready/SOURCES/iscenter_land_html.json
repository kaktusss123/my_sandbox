      },
      "seller_name": {
        "type": "string",
        "path": "//div[@class='dit']//text()[contains(.,'Агент:')]/following-sibling::a/text()[normalize-space()]"
      },
      "seller_company": {
        "type": "string",
        "path": "//div[@class='dit']//text()[contains(.,'АН:')]/following-sibling::a/text()[normalize-space()]"
      },
      "phone": {
        "type": "phone",
        "path": "//a[contains(@href,'tel:')]/text()[normalize-space()]"
      },
      "description": {
        "type": "list to string",
