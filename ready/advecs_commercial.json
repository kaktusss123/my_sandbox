{
    "_comment": "был мусорный ключ tables в item_page без указания path + увеличено количество потоков",
    "_task_id": "ADVEKS_TEST_1",
    "_start_urls": [
        "https://www.advecs.com/catalog/commercial/"
    ],
    "_segment": "Коммерческая Недвижимость",
    "_operation": "",
    "_federal_subject": "",
    "_source": "http://www.advecs.com",
    "project_id": "metaspider",
    "spider_id": "metacrawler",
    "global_settings": {
        "CONCURRENT_REQUESTS": 1024,
        "SPLASH_URL": "http://0.0.0.0:8050",
        "LOG_LEVEL": "INFO",
        "AUTOTHROTTLE_ENABLED": false,
        "AUTOTHROTTLE_START_DELAY": 1,
        "AUTOTHROTTLE_MAX_DELAY": 60,
        "AUTOTHROTTLE_TARGET_CONCURRENCY": 1024,
        "CONCURRENT_REQUESTS_PER_DOMAIN": 1024,
        "RETRY_ENABLED": true,
        "RETRY_HTTP_CODES": [
            500,
            503,
            502,
            504,
            400,
            403,
            408,
            429,
            505,
            303,
            302,
            301,
            404,
            307,
            410,
            502,
            451
        ],
        "HTTPERROR_ALLOWED_CODES": [],
        "RETRY_TIMES": 20,
        "ROBOTSTXT_ENABLED": false,
        "ROBOTSTXT_OBEY": false,
        "REDIRECT_ENABLED": false,
        "COOKIES_ENABLED": false,
        "DOWNLOAD_FAIL_ON_DATALOSS": false,
        "RANDOM_UA_TYPE": "random",
        "DOWNLOAD_TIMEOUT": 60,
        "CLOSESPIDER_ITEMCOUNT": 0,
        "CLOSESPIDER_ERRORCOUNT": 0
    },
    "methods": {
        "crawling_type": "common",
        "allowed_domains": [],
        "proxy_settings": {
            "types": [
                "https"
            ],
            "amount": 10000,
            "save": false
        }
    },
    "crawl_tree": [{
        "pagination_urls": {
            "path": "(//div[@class = 'pages']/span/following-sibling::a[1]/@href)[1]"
        },
        "item_urls": {
            "path": "//td[@class = 'first']//@href",
            "methods": {"filter_duplicates": false}
        }
    }],
    "item_page": {
        "fields": {
            "source_latitude": {
                "type": "float",
                "path": "//script[contains(., 'LAT')]",
                "regex": "(?<=LAT\":\")([^;]*)(?=\",\"LNG)",
                "operations": []
            },
            "address": {
                "type": "list to string",
                "path": "//span[@class ='title indentright']//text()",
                "operations": []
            },
            "purpose": {
                "type": "string",
                "path": "//div[contains(@id, 'offer')]/h3/text()",
                "operations": []
            },
            "source_longitude": {
                "type": "float",
                "path": "//script[contains(., 'LAT')]",
                "regex": "(?<=LNG\":\")([^;]*)(?=\",\"ZOOM)",
                "operations": []
            },
            "area_total": {
                "type": "string",
                "path": "//div[@id='boxBlock2']/div/text()",
                "operations": []
            },
            "floors_cnt": {
                "type": "string",
                "path": "(//div[@class = 'area']/text())[2]",
                "regex": "(?<=\\/)\\d+",
                "operations": []
            },
            "date_info": {
                "type": "date",
                "time": "now",
                "operations": []
            },
            "operation": {
                "type": "string",
                "path": "//div[contains(@id, 'offer')]/h1/text()",
                "operations": []
            },
            "date_created": {
                "type": "date",
                "path": "(//td[@id = 'right']/div[@align ='right']/text())[1]",
                "time": "from string",
                "regex": "(?<=введена )([^;]*)(?=\\,)",
                "operations": []
            },
            "title_name": {
                "type": "list to string",
                "path": "//span[@class ='title indentright']//text()",
                "operations": []
            },
            "price_total": {
                "type": "list to string",
                "path": "//div[@class ='price']//text()",
                "regex": "\\d+",
                "operations": []
            },
            "federal_subject": {
                "type": "string",
                "value": "Санкт-Петербург г.",
                "operations": []
            },
            "description": {
                "type": "list to string",
                "path": "//div[@class = 'discharged2 serif']/text()",
                "operations": []
            },
            "address_city": {
                "type": "string",
                "path": "//div[@class = 'currentCity']",
                "operations": []
            },
            "floor": {
                "type": "string",
                "path": "(//div[@class = 'area']/text())[2]",
                "regex": "(?<=Этаж: )\\d+(?=\\/)",
                "operations": []
            },
            "phone": {
                "type": "phone",
                "path": "//div[@class='serif bigFont']/text()",
                "operations": []
            },
            "seller_name": {
                "type": "string",
                "path": "//div[@id='boxBlock3']/a[1]/text()",
                "operations": []
            },
            "price_currency": {
                "type": "string",
                "path": "//div[@class='price']/text()[normalize-space()]",
                "operations": []
            }
        },
        "tables": {
            "table": {
                "_keys_path": "//div[@class='discharged']/strong/text()[normalize-space()]",
                "_vals_path": "//div[@class='discharged']/text()[normalize-space()]",
                "key_regex": "\\w+",
                "key_converter": {
                    "этажей": {
                        "type": "string",
                        "name": "floors_cnt"
                    },
                    "ремонт": {
                        "type": "string",
                        "name": "decoration_type"
                    },
                    "вход": {
                        "type": "string",
                        "name": "private_entrance"
                    },
                    "электричество": {
                        "type": "string",
                        "name": "is_electricity"
                    },
                    "водоснабжение": {
                        "type": "string",
                        "name": "is_plumbing"
                    },
                    "отопление": {
                        "type": "string",
                        "name": "heat_warehouse"
                    },
                    "назначение": {
                        "type": "string",
                        "name": "purpose"
                    },
                    "тип_постройки": {
                        "type": "string",
                        "name": "building_type"
                    },
                    "этаж": {
                        "type": "string",
                        "name": "floor",
                        "regex": "(.*)(?=/)"
                    }
                }
            }
        }
    }
}