{
  "_comment": "",
  "_task_id": "ALIANT_test_1",
  "_start_urls": [
    "https://aliant.pro/sitemap.xml"
  ],
  "_segment": "Коммерческая недвижимость",
  "_operation": "",
  "_federal_subject": "from affress_city",
  "_source": "aliant.pro",
  "global_settings": {
    "CONCURRENT_REQUESTS": 128,
    "CONCURRENT_REQUESTS_PER_DOMAIN": 128,
    "DOWNLOAD_TIMEOUT": 20,
    "SPLASH_URL": "http://0.0.0.0:8050",
    "LOG_LEVEL": "INFO",
    "AUTOTHROTTLE_ENABLED": false,
    "AUTOTHROTTLE_START_DELAY": 1.0,
    "AUTOTHROTTLE_MAX_DELAY": 60,
    "AUTOTHROTTLE_TARGET_CONCURRENCY": 64,
    "HTTPERROR_ALLOWED_CODES": [],
    "RETRY_ENABLED": true,
    "RETRY_TIMES": 20,
    "RETRY_HTTP_CODES": [
      500,
      503,
      504,
      505,
      303,
      302,
      301,
      307,
      410
    ],
    "ROBOTSTXT_ENABLED": false,
    "ROBOTSTXT_OBEY": false,
    "REDIRECT_ENABLED": true,
    "COOKIES_ENABLED": true,
    "RANDOM_UA_TYPE": "random"
  },
  "methods": {
    "crawling_type": "common",
    "allowed_domains": [],
    "proxy_settings": {
      "types": [
        "https"
      ],
      "amount": 10000
    }
  },
"sitemap_urls": {
  "start": [
    "https://aliant.pro/sitemap.xml"
  ],
  "type": "sitemap",
  "target": "sitemap",
  "crawl_tree": {
      "sitemap": {
      "targets": {
        "item": {
          "type": "item",
          "filters": [
            "aliant.pro/catalog/commercial/ul", "aliant.pro/catalog/commercial/g", "aliant.pro/catalog/commercial/l"
          ]
        }
      }},
    "item": {}
  }},
  "item_page": {
    "methods": {
      "crawling_type": "common",
      "filter_duplicates": true
    },
    "fields": {
      "date_info": {
        "type": "date",
        "time": "now"
      },
      "price_currency": {
        "type": "string",
        "value": "RUB"
      },
      "title_name": {
        "type": "string",
        "path": "//h1//text()"
      },
      "company_name": {
        "type": "string",
        "value": "ООО «Элиант Недвижимость»"
      },
      "email_seller": {
        "type": "string",
        "value": "aliantrealty@gmail.com"
      },
      "phone": {
        "type": "list to string",
        "path": "//div[@class='recallBlock']/a/@href",
        "regex": "(?<=tel:).+"
      },
      "description": {
        "type": "list to string",
        "path": "//div[@class='col l8 s12']//p//text()"
      },
      "longitude": {
        "type": "float",
        "path": "//div[@class='row itemDescription']/@data-coords",
        "regex": ",(\\d*\\W?\\d+\\W?\\d+)"
      },
      "latitude": {
        "type": "float",
        "path": "//div[@class='row itemDescription']/@data-coords",
        "regex": "(\\d*\\W?\\d+\\W?\\d+),"
      },
      "decoration_type": {
        "type": "string",
        "path": "//div[text()[contains(.,'Состояние')]]//span//text()"
      },
      "private_entrance": {
        "type": "string",
        "path": "//div[text()[contains(.,'Отдельный')]]//span//text()"
      },
      "parking_type": {
        "type": "string",
        "path": "//div[text()[contains(.,'Парковка')]]//span//text()"
      },
      "ceiling_height": {
        "type": "string",
        "path": "//div[text()[contains(.,'Высота')]]//span//text()"
      },
      "ventilation_system": {
        "type": "string",
        "path": "//div[text()[contains(.,'Вентиляция')]]//span//text()"
      },
      "contract_type": {
        "type": "string",
        "path": "//div[text()[contains(.,'Тип договора')]]//span//text()"
      },
      "security_type": {
        "type": "string",
        "path": "//div[text()[contains(.,'Безопасность')]]//span//text()"
      },
      "nds": {
        "type": "string",
        "path": "//div[text()[contains(.,'Налоги')]]//span//text()"
      },
      "address": {
        "type": "string",
        "path": "//h1[@class = 'title mainTitle']/text()"
      },
      "count_phone_lines": {
        "type": "string",
        "path": "//li[text()[contains(.,'Телекоммуникации')]]//text()",
        "regex": ": (.+)"
      },
      "parking_cnt": {
        "type": "string",
        "path": "//li[text()[contains(.,'Парковка')]]//text()",
        "regex": "\\d*\\W?\\d+"
      },
      "area_total": {
        "type": "string",
        "path": "//div[@class = 'specification characteristics']/div[contains(.,'площадь')]/span/text()",
        "regex": "\\d+\\W?\\d+"
      },
      "condition": {
        "type": "string",
        "path": "//li[text()[contains(.,'Кондиционирование')]]//text()",
        "regex": ": (.+)"
      }
    },
    "tables": {
      "table_1": {
        "_keys_path": "//div[@class='specification characteristics']//div/text()",
        "_vals_path": "//div[@class='specification characteristics']//div//span//text()",
        "key_converter": {
          "город": {
            "name": "address_city",
            "type": "string"
          },
          "номер_дома": {
            "name": "address_number",
            "type": "string"
          },
          "общая_площадь": {
            "name": "area_total",
            "type": "string"
          },
          "район": {
            "name": "address_district",
            "type": "string"
          },
          "тип_сделки": {
            "name": "operation",
            "type": "string"
          },
          "улица": {
            "name": "address_street",
            "type": "string"
          },
          "цена": {
            "name": "price",
            "type": "string"
          },
          "этаж": {
            "name": "floor_room",
            "type": "string"
          }
        }
      }
    }
  }
}
